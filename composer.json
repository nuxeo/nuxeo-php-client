{
  "name": "nuxeo/nuxeo-php-client",
  "description": "PHP Client Library for the Nuxeo Automation API",
  "keywords": ["nuxeo", "automation", "client"],
  "homepage": "http://www.nuxeo.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Pierre-Gildas MILLON",
      "email": "pgmillon@nuxeo.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "require": {
    "php": "~7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0, <6.6",
    "monolog/monolog": "~1.3, <1.28",
    "laminas/laminas-mail": "~2.12, <2.13",
    "jms/serializer": "~1.8, <1.15"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1, <7.6",
    "symfony/http-foundation": "~4.1, <4.5",
    "enlightn/security-checker": "~1.10.0"
  },
  "autoload": {
    "psr-4": {
      "Nuxeo\\": "src/Nuxeo",
      "Laminas\\Mime\\": "src/Laminas/Mime"
    }
  },
  "replace": {
    "laminas/laminas-mime": "^2.5"
  },
  "autoload-dev" : {
    "psr-4": {
      "Nuxeo\\Client\\Tests\\": "tests/Nuxeo/Client/Tests",
      "Nuxeo\\Client\\FTests\\": "ftests/Nuxeo/Client/FTests",
      "LaminasTest\\Mime\\": "tests/LaminasTest/Mime"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pgmillon/laminas-mime.git"
    }
  ]
}
